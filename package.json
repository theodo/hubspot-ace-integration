{
  "name": "hubspot-ace-integration",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "sls deploy --aws-profile ace-integration-staging --stage staging",
    "list-results": "aws s3 ls s3://ace-apn-1588143-beta-us-west-2/opportunity-inbound-processed-results/ --profile apn-crm-integration-staging-assume-role",
    "list-outbound": "aws s3 ls s3://ace-apn-1588143-beta-us-west-2/opportunity-outbound/ --profile apn-crm-integration-staging-assume-role",
    "list-outbound-lead": "aws s3 ls s3://ace-apn-1588143-beta-us-west-2/lead-outbound/ --profile apn-crm-integration-staging-assume-role",
    "get-result": "func() { aws s3 cp s3://ace-apn-1588143-beta-us-west-2/opportunity-inbound-processed-results/\"$1\" ./testing/results/\"$1\" --profile apn-crm-integration-staging-assume-role --acl bucket-owner-full-control; }; func",
    "post-inbound": "func() { aws s3 cp ./testing/post-opportunity/\"$1\" s3://ace-apn-1588143-beta-us-west-2/opportunity-inbound/\"$1\" --profile apn-crm-integration-staging-assume-role --acl bucket-owner-full-control; }; func",
    "get-outbound": "func() { aws s3 cp s3://ace-apn-1588143-beta-us-west-2/opportunity-outbound/\"$1\" ./testing/outbound-opportunity/\"$1\" --profile apn-crm-integration-staging-assume-role --acl bucket-owner-full-control; }; func",
    "get-outbound-lead": "func() { aws s3 cp s3://ace-apn-1588143-beta-us-west-2/lead-outbound/\"$1\" ./testing/lead-outbound/\"$1\" --profile apn-crm-integration-staging-assume-role --acl bucket-owner-full-control; }; func"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.44.0",
    "@aws-sdk/credential-providers": "^3.45.0",
    "@hubspot/api-client": "^5.0.0",
    "@middy/core": "^2.5.3",
    "@middy/http-json-body-parser": "^2.5.3",
    "@middy/ssm": "^2.5.4",
    "aws-sdk": "^2.1048.0",
    "axios": "^0.24.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "typebridge": "^0.7.1"
  },
  "devDependencies": {
    "@aws-cdk/aws-events": "^1.137.0",
    "@aws-cdk/aws-iam": "^1.137.0",
    "@aws-cdk/aws-sqs": "^1.138.0",
    "@aws-cdk/aws-ssm": "^1.137.0",
    "@aws-cdk/core": "^1.137.0",
    "@serverless/typescript": "^3.2.1",
    "@types/aws-lambda": "^8.10.71",
    "@types/jest": "^27.0.3",
    "@types/node": "^14.14.25",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "esbuild": "^0.14.6",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.5",
    "json-schema-to-ts": "^1.5.0",
    "prettier": "^2.5.1",
    "serverless": "^3.3.0",
    "serverless-esbuild": "^1.17.1",
    "serverless-lift": "^1.11.0",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.5.4"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
